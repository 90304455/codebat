
public class Name {

	public static void main(String[] args) {
		String("hellell")

	}
	@SuppressWarnings("unused")
	public boolean stringE(String str) {
		  int y=0;
		  for(int i = 0;i<str.length(); i=i+1){
		      if(str.substring(i,i+1).equals("e")){
		      y++;
		      }
		   if(y<=3 && y>=1){
		   return true;
		   }
		   else{
		   return false;
		   }
		  }
		return false; 
		}

}
